{
  "name": "vscode-dev-conf",
  "displayName": "DevConf example",
  "description": "",
  "version": "0.0.1",
  "publisher": "litleleprikon",
  "engines": {
    "vscode": "^1.30.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:ping.ping"
  ],
  "main": "./out/src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "ping.ping",
        "title": "Ping Host"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "yarn run compile",
    "compile": "yarn build",
    "build": "tsc -p ./",
    "build:watch": "tsc -watch -p ./",
    "postinstall": "node ./node_modules/vscode/bin/install",
    "test": "yarn run compile && node ./node_modules/vscode/bin/test",
    "start": "node dist/src/server.js",
    "lint:editorconfig": "yarn editorconfig-checker --exclude-regexp 'coverage|out|node_modules|yarn*' ./",
    "lint:ts": "yarn tslint -c tslint.json ./{src,test}/**/*.ts",
    "lint": "yarn lint:ts && yarn lint:editorconfig"
  },
  "devDependencies": {
    "@types/glob": "^7.1.1",
    "@types/istanbul": "^0.4.30",
    "@types/mocha": "^5.2.5",
    "@types/node": "^10.12.18",
    "@types/source-map-support": "^0.4.1",
    "decache": "^4.5.0",
    "editorconfig-checker": "^1.3.3",
    "glob": "^7.1.3",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "remap-istanbul": "^0.13.0",
    "source-map-support": "^0.5.10",
    "tslint": "^5.12.1",
    "typescript": "^3.2.2",
    "vsce": "^1.54.0",
    "vscode": "^1.1.26"
  }
}
